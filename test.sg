Values :: struct
{
    a: i32;
	b: i32;
	c: i32;
}

GLOBAL_VALUE :: 3;

mul :: (values: Values): i32
{
	test :: (): i32 {
		if values.a == 0 && values.b == 0 {
			return 420;
		}

		return 0;
	}

	if test() == 420 {
		return 0
	}

	return values.a * values.b * values.c;
}

main :: (): i32
{
	s := Values { 
		a: 11,
		b: 22, 
		c: 33,
	};

	if { cond := s.a < 10; cond } {
		return 0;
	}

	printf("%d |", mul(s));

	s.a = 0;
	s.b = 0;

	res := mul(s);

	printf(" %d", res);

	0
}

