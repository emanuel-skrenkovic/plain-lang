Values :: struct
{
    a: i32;
	b: i32;
	c: i32;
}

sum :: (v: Values): i32
{
	v.a + v.b + v.c + GLOBAL_VALUE
}

GLOBAL_VALUE :: 3;

main :: (): i32
{
	s := Values { 
		a: 11, 
		b: 22, 
		c: 33,
	};

	result1 := sum(s);

	printf("%d |", result1);

	s.a = 1;
	s.b = 1;
	s.c = 1;

	result2 := sum(s);

	printf("| %d", result2 + GLOBAL_VALUE);

   0
}

