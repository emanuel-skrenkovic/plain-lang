add2 :: (a: number, b: number)
{
    c :: if a > b {
        if b + 3 > a {
            a + b + 15
        } else {
          0
        }
    } else {
        a - b
    };

    d := 0;
    while d < 15 {
        d = d + 1;
    }

    a + b + c + d
}

result :: add2(3, 1);
f_str :: "%d\0";
printf(f_str, result);
