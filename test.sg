Values :: struct
{
    a: i32;
	b: i32;
	c: i32;
}

GLOBAL_VALUE :: 3;

mul :: (values: Values): i32
{
	test :: (): i32 {
		if values.a == 0 {
			if values.b == 0 {
				return 420
			}
		}	

		return 0
	}

	test_result := test();
	if test_result == 420 {
		return 0
	}

	return values.a * values.b * values.c
}

main :: (): i32
{
	s := Values { 
		a: 11,
		b: 22, 
		c: 33,
	};

	if s.a < 10 {
		return 0;
	}

	s.a = 0;
	s.b = 0;

	res := mul(s);

	printf(" %d", res);

	0
}

