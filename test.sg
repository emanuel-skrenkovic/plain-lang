Inner :: struct
{
	a: i32;
}

Values :: struct
{
    a: i32;
	b: i32;
	c: i32;
	d: Inner;
}

Values :: sum :: (self): i32 
{ 
	test :: (): i32 
	{
		self.a + self.b + self.c
	};

	test()
}

Values :: mul :: (self): i32
{
	self.a * self.b * self.c
}

main :: (): i32
{
	s := Values { 
		a: 1,
		b: 2, 
		c: 3,
		d: Inner {
		    a: 420,
		},
	};

	result :: s.sum() + s.mul();

	printf("%d", result);

	0
}

