test :: (a: number, b: number)
{
    result := 0;

    for i := 0; i <= 15; i = i + 1 {
        result = result + i;
    }

    j := 15;

    while j >= 0 {
        result = result - j;
        j = j - 1;
    }

    a + b - result
}

main :: ()
{
    result := test(1, 2);
    result = result + 13;

    add :: () { result + 14 };

    printf("%d", add());

    0
}
